package dto;

public class MoviesDTO {
	private int id;
	private String title;
	private String ganre;
	
	public MoviesDTO(){
		
	}
	
	public MoviesDTO(int id, String title, String ganre) {
		this.id = id;
		this.title = title;
		this.ganre = ganre;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getTitle() {
		return title;
	}

	public void setTitle(String title) {
		this.title = title;
	}

	public String getGanre() {
		return ganre;
	}

	public void setGanre(String ganre) {
		this.ganre = ganre;
	}
	
}
